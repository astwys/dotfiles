# This is Git's per-user configuration file.
[user]
	name = astwys
	email = astwys@gmail.com
[push]
	default = simple
[credential]
	helper = osxkeychain
[core]
	editor = subl -n -w
	excludesfile = /Users/michael/.gitignore_global
[gui]
	recentrepo = /Users/michael/sandbox/Garlic
[alias]
	s = status
	summary = status -u -s
	save = commit -am
	graph = log --graph -10 --branches --remotes --tags  --format=format:'%Cgreen%h %Cresetâ€¢ %<(75,trunc)%s (%cN, %cr) %Cred%d' --date-order
	what = show -w
	go = checkout
	newbranch = checkout -b
	branches = branch -a
	up = pull --rebase --autostash
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
